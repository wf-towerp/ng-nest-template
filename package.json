{
    "name": "ng14-nest-template",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "predev:ssr": "npx mkdirp ./dist/ng14-nest-template/config && cp ./environments/.env.local ./dist/ng14-nest-template/config/.env",
        "dev:ssr": "ng run ng14-nest-template:serve-ssr",
        "serve:ssr": "node dist/ng14-nest-template/server/main.js",
        "prebuild:ssr": "npx mkdirp ./dist/ng14-nest-template/config && cp ./environments/.env.production ./dist/ng14-nest-template/config/.env && ngcc",
        "build:ssr": "ng build && ng run ng14-nest-template:server",
        "prerender": "ng run ng14-nest-template:prerender"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "14.2.1",
        "@angular/common": "14.2.1",
        "@angular/compiler": "14.2.1",
        "@angular/core": "14.2.1",
        "@angular/forms": "14.2.1",
        "@angular/platform-browser": "14.2.1",
        "@angular/platform-browser-dynamic": "14.2.1",
        "@angular/platform-server": "14.2.1",
        "@angular/router": "14.2.1",
        "@angular/service-worker": "14.2.1",
        "@nestjs/common": "9.0.11",
        "@nestjs/core": "9.0.11",
        "@nestjs/jwt": "9.0.0",
        "@nestjs/ng-universal": "5.0.2",
        "@nestjs/passport": "9.0.0",
        "@nestjs/platform-express": "9.0.11",
        "@nestjs/typeorm": "8.0.3",
        "@nguniversal/express-engine": "14.2.0",
        "bcrypt": "5.0.1",
        "class-transformer": "0.2.3",
        "class-validator": "0.9.1",
        "cookie-parser": "1.4.6",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "mysql2": "2.3.3",
        "passport": "0.5.2",
        "passport-jwt": "4.0.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.6",
        "tslib": "2.3.0",
        "typeorm": "0.2.45",
        "zone.js": "0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.2.2",
        "@angular/cli": "14.2.2",
        "@angular/compiler-cli": "14.2.1",
        "@nguniversal/builders": "12.1.1",
        "@types/cookie-parser": "1.4.2",
        "@types/express": "4.17.13",
        "@types/jasmine": "4.0.3",
        "@types/node": "18.7.16",
        "@types/passport-jwt": "3.0.6",
        "jasmine-core": "4.3.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage": "2.2.0",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.7.2"
    }
}
