{
    "name": "ng-nest-template",
    "version": "1.1.0",
    "scripts": {
        "ng": "ng",
        "setup": "npm install --force",
        "test": "ng test",
        "predev:ssr": "npx mkdirp ./dist/config && cp ./src/environments/.env.local ./dist/config/.env",
        "dev:ssr": "ng run main-app:serve-ssr",
        "serve:ssr": "node dist/server/main.js",
        "prebuild:ssr": "npx mkdirp ./dist/config && cp ./src/environments/.env.production ./dist/config/.env && ngcc",
        "build:ssr": "ng build && ng run main-app:server",
        "prerender": "ng run main-app:prerender",
        "typeorm:cli:old": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/server/config/typeorm-migrations.config.ts",
        "pretypeorm:cli": "npx mkdirp ./dist/config && cp ./src/environments/.env.local ./dist/config/.env",
        "typeorm:cli": "typeorm-ts-node-commonjs",
        "migrations:generate": "npm run migrations:generate:core && npm run migrations:generate:tenants",
        "migrations:generate:core": "npm run typeorm:cli -- migration:generate src/server/migrations/core/myapp -d ./src/server/config/migrations-core.config.ts",
        "migrations:generate:tenants": "npm run typeorm:cli -- migration:generate src/server/migrations/tenants/myapp -d ./src/server/config/migrations-tenants.config.ts",
        "migrations:run:core": "npm run typeorm:cli -- migration:run -d ./src/server/config/migrations-core.config.ts",
        "migrations:revert:core": "npm run typeorm:cli -- migration:revert -d ./src/server/config/migrations-core.config.ts"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "14.2.7",
        "@angular/common": "14.2.7",
        "@angular/compiler": "14.2.7",
        "@angular/core": "14.2.7",
        "@angular/forms": "14.2.7",
        "@angular/platform-browser": "14.2.7",
        "@angular/platform-browser-dynamic": "14.2.7",
        "@angular/platform-server": "14.2.7",
        "@angular/router": "14.2.7",
        "@angular/service-worker": "14.2.7",
        "@nestjs/common": "9.0.11",
        "@nestjs/core": "9.0.11",
        "@nestjs/jwt": "9.0.0",
        "@nestjs/ng-universal": "5.0.2",
        "@nestjs/passport": "9.0.0",
        "@nestjs/platform-express": "9.0.11",
        "@nestjs/typeorm": "9.0.1",
        "@nguniversal/express-engine": "14.2.0",
        "bcrypt": "5.0.1",
        "class-transformer": "0.2.3",
        "class-validator": "0.9.1",
        "cookie-parser": "1.4.6",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "mysql2": "2.3.3",
        "passport": "0.5.2",
        "passport-jwt": "4.0.0",
        "pg": "8.8.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.6",
        "tslib": "2.3.0",
        "typeorm": "0.3.10",
        "zone.js": "0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.2.6",
        "@angular/cli": "14.2.6",
        "@angular/compiler-cli": "14.2.7",
        "@nguniversal/builders": "12.1.1",
        "@types/cookie-parser": "1.4.2",
        "@types/express": "4.17.13",
        "@types/gulp": "4.0.9",
        "@types/jasmine": "4.0.3",
        "@types/node": "18.7.16",
        "@types/passport-jwt": "3.0.6",
        "@types/webpack-env": "1.18.0",
        "gulp": "4.0.2",
        "jasmine-core": "4.3.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage": "2.2.0",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.0.0",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.7.2"
    }
}
