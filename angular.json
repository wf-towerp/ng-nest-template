{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "src",
    "projects": {
        "main-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src/browser",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/browser",
                        "index": "src/browser/index.html",
                        "main": "src/browser/main.ts",
                        "polyfills": "src/browser/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/browser/favicon.ico",
                            "src/browser/assets",
                            "src/browser/manifest.webmanifest"
                        ],
                        "styles": [
                            "src/browser/styles.scss"
                        ],
                        "scripts": [],
                        "serviceWorker": true,
                        "ngswConfigPath": "ngsw-config.json"
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/browser/environments/environment.ts",
                                    "with": "src/browser/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "main-app:build:production"
                        },
                        "development": {
                            "browserTarget": "main-app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "main-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/browser/test.ts",
                        "polyfills": "src/browser/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/browser/favicon.ico",
                            "src/browser/assets",
                            "src/browser/manifest.webmanifest"
                        ],
                        "styles": [
                            "src/browser/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "outputPath": "dist/server",
                        "main": "src/server.ts",
                        "tsConfig": "tsconfig.server.json",
                        "inlineStyleLanguage": "scss",
                        "externalDependencies": [
                            "@nestjs/microservices",
                            "@nestjs/microservices/microservices-module",
                            "@nestjs/websockets",
                            "@nestjs/websockets/socket-module",
                            "cache-manager",
                            "bcrypt",
                            "pg"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "outputHashing": "media",
                            "fileReplacements": [
                                {
                                    "replace": "src/browser/environments/environment.ts",
                                    "with": "src/browser/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": false
                        },
                        "development": {
                            "optimization": false,
                            "sourceMap": true,
                            "extractLicenses": false
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve-ssr": {
                    "builder": "@nguniversal/builders:ssr-dev-server",
                    "configurations": {
                        "development": {
                            "browserTarget": "main-app:build:development",
                            "serverTarget": "main-app:server:development"
                        },
                        "production": {
                            "browserTarget": "main-app:build:production",
                            "serverTarget": "main-app:server:production"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "prerender": {
                    "builder": "@nguniversal/builders:prerender",
                    "options": {
                        "routes": [
                            "/"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "main-app:build:production",
                            "serverTarget": "main-app:server:production"
                        },
                        "development": {
                            "browserTarget": "main-app:build:development",
                            "serverTarget": "main-app:server:development"
                        }
                    },
                    "defaultConfiguration": "production"
                }
            }
        }
    }
}
